    "import math\n",
    "\n",
    "def f(x):\n",
    "    return math.cos(x) - math.sin(x)\n",
    "    \n",
    "x_l = 0\n",
    "x_u = 1\n",
    "\n",
    "while(abs(f(x)) > 0.001):\n",
    "    \n",
    "    x = (x_l + x_u) / 2\n",
    "    \n",
    "    if f(x) > 0:\n",
    "        x_l = x\n",
    "    else:\n",
    "        x_u = x\n",
    "    \n",
    "print(x, f(x))"
   ]
